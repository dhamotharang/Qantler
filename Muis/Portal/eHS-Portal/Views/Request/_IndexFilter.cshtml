<div class="modal fade filter" id="filterModal" role="dialog" aria-labelledby="filterModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Advanced Filter</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form class="form">
                    <input type="hidden" id="filterCustomerId" />
                    <div class="form-group row">
                        <label class="col-sm-3 col-form-label">Request #</label>
                        <div class="col-sm-9">
                            <input id="filterID" type="text" class="form-control form-control-lg" maxlength="10">
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-3 col-form-label">Customer Code</label>
                        <div class="col-sm-9">
                            <input id="filterCustomerCode" type="text" class="form-control form-control-lg" maxlength="36">
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-3 col-form-label">Customer</label>
                        <div class="col-sm-9">
                            <input id="filterCustomer" type="text" class="form-control form-control-lg" maxlength="150">
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-3 col-form-label">Premise</label>
                        <div class="col-sm-9">
                            <input id="filterPremise" type="text" class="form-control form-control-lg" maxlength="700">
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-3 col-form-label">Type</label>
                        <div class="col-sm-9">
                            <select id="filterTypeSelect" class="select-multiple select2-hidden-accessible" multiple="multiple" style="width:100%" aria-hidden="true">
                                <option value="0">New</option>
                                <option value="1">Renewal</option>
                                <option value="2">HC01</option>
                                <option value="3">HC02</option>
                                <option value="4">HC03</option>
                                <option value="5">HC04</option>
                                <option value="6">HC05</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-3 col-form-label">Status</label>
                        <div class="col-sm-9">
                            <select id="filterStatusSelect" class="select-multiple select2-hidden-accessible" multiple="multiple" style="width:100%" aria-hidden="true">
                                <option value="100">Draft</option>
                                <option value="200">Open</option>
                                <option value="250">Pending Review Approval</option>
                                <option value="300">Scheduled For Inspection</option>
                                <option value="400">For Approval</option>
                                <option value="550">For Mufti Acknowledgement</option>
                                <option value="600">Pending Bill</option>
                                <option value="700">Pending Payment</option>
                                <option value="800">Issuance</option>
                                <option value="900">Closed</option>
                                <option value="1000">Rejected</option>
                                <option value="1100">Cancelled</option>
                                <option value="1200">Expired</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group row" id="divfilterStatusMinor">
                        <label class="col-sm-3 col-form-label">Status Minor</label>
                        <div class="col-sm-9">
                            <select id="filterStatusMinorSelect" class="select-multiple select2-hidden-accessible" multiple="multiple" style="width:100%" aria-hidden="true">
                                <option value="120">Setup Customer Code</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-3 col-form-label">RFA Status</label>
                        <div class="col-sm-9">
                            <select id="filterRFAStatusSelect" class="select-single select2-hidden-accessible" style="width:100%" aria-hidden="true">
                                <option></option>
                                <option value="1">Draft</option>
                                <option value="100">Open</option>
                                <option value="200">Pending Review</option>
                                <option value="300">Closed</option>
                                <option value="400">Expired</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-3 col-form-label">Manage By</label>
                        <div class="col-sm-9">
                            <select id="filterUserSelect" class="select-multiple select2-hidden-accessible" multiple="multiple" style="width:100%" aria-hidden="true">
                            </select>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-3 col-form-label">Escalate Status</label>
                        <div class="col-sm-9">
                            <select id="filterEscalateStatusSelect" class="select-single select2-hidden-accessible" style="width:100%" aria-hidden="true">
                                <option></option>
                                <option value="100">Open</option>
                                <option value="200">Closed</option>>
                            </select>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-3 col-form-label">From</label>
                        <div class="col-sm-9">
                            <div id="datepicker-from" class="input-group date datepicker">
                                <input type="text" class="form-control">
                                <span class="input-group-addon input-group-append border-left">
                                    <span class="mdi mdi-calendar input-group-text"></span>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-3 col-form-label">To</label>
                        <div class="col-sm-9">
                            <div id="datepicker-to" class="input-group date datepicker">
                                <input type="text" class="form-control">
                                <span class="input-group-addon input-group-append border-left">
                                    <span class="mdi mdi-calendar input-group-text"></span>
                                </span>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button id="filterApplyButton" type="button" class="btn btn-outline-primary">Apply</button>
                <button id="filterClearButton" type="button" class="btn btn-outline-secondary">Clear</button>
                <button id="filterResetButton" type="button" class="btn btn-outline-secondary">Reset</button>
                <button id="filterCloseButton" type="button" class="btn btn-outline-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>