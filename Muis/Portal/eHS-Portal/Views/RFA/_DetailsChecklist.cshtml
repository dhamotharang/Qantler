@model eHS.Portal.Model.RFA;

<div class="card checklist">
    <div class="card-body">
        <div class="row">
            <div class="col-12">
                <ul class="nav nav-tabs tab-solid tab-solid-danger" role="tablist">
                </ul>
                <div class="tab-content tab-content-solid">
                </div>
            </div>
        </div>
    </div>

    <div class="template" hidden>
        <li class="nav-item">
            <a class="nav-link {active}" id="tab-{id}" data-toggle="tab" href="#pane-{id}" role="tab">{text}</a>
        </li>

        <div class="tab-pane fade" id="pane-{id}" role="tabpanel">
        </div>
    </div>
</div>


